{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev:next": {
      "cache": false,
      "persistent": true
    },
    "build:next": {
      "dependsOn": [],
      "inputs": ["package.json", "src/**/*"],
      "outputs": ["dist/**/*", ".next/**/*", "!.next/cache/**/*", ".turbo/**/*"]
    }
  }
}
